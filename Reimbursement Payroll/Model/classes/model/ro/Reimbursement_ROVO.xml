    reimbursement_V.BUSINESS_UNIT_ID BUSINESS_UNIT_ID,     
    reimbursement_V.PERSON_ID PERSON_ID,  
    legal_entities.name  
FROM     
    XXSSHR_REIMBURSEMENT_REQUEST_V reimbursement_V,  
    xxper_employee_v             employee_V,  
    xxhr_legal_entities          legal_entities  
WHERE  
        employee_V.legal_entity_id = legal_entities.organization_id  
    AND reimbursement_V.person_id = employee_V.person_id]]></SQLQuery>
